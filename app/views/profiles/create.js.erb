
var formHandle = document.getElementById('new_profile');
var errorHandle = document.getElementById('errors');
<% if @profile.errors.any? %>
	errorHandle.innerHTML = "<p><%= @profile.errors.full_messages.join(',') %></p>";
<% else %>
formHandle.style.display = "none";
formHandle.outerHTML = "<h3><%= @profile.bio %></h3>";
<% end %>
